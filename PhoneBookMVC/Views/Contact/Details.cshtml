@model PhoneBookLib.Contact

@{
    ViewData["Title"] = "Details";
}
<div class="rows">
    @Html.Partial("View")
    <div class="row3">

<h1>Details</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Id
        </dt>
        <dd class = "col-sm-10">
            @Model.Id
        </dd>
        <dt class = "col-sm-2">
            FirstName
        </dt>
        <dd class = "col-sm-10">
            @Model.FirstName
        </dd>
        <dt class = "col-sm-2">
            LastName
        </dt>
        <dd class = "col-sm-10">
            @Model.LastName
        </dd>
        <dt class = "col-sm-2">
            Gender
        </dt>
        <dd class = "col-sm-10">
            @Model.Gender
        </dd>
        <dt class = "col-sm-2">
            Dob
        </dt>
        <dd class = "col-sm-10">
            @Model.Dob
        </dd>
        <dt class = "col-sm-2">
            Email
        </dt>
        <dd class = "col-sm-10">
            @Model.Email
        </dd>
        <dt class = "col-sm-2">
            Phone
        </dt>
        <dd class = "col-sm-10">
            @Model.Phone
        </dd>
        <dt class = "col-sm-2">
            City
        </dt>
        <dd class = "col-sm-10">
            @Model.City
        </dd>
        <dt class = "col-sm-2">
            State
        </dt>
        <dd class = "col-sm-10">
            @Model.State
        </dd>
        <dt class = "col-sm-2">
            Country
        </dt>
        <dd class = "col-sm-10">
            @Model.Country
        </dd>
         <dt class="col-sm-2">
            <img src=@Model.Picture id="detailimg" />
         </dt>
    </dl>
</div>
<div>
    @Html.ActionLink("Home", "Home") |
    <a asp-action="Home">Back to List</a>
</div>
</div>
</div>